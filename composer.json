{
    "name": "starychenko/catalog-master",
    "description": "WordPress plugin for managing product catalogs with Google Sheets integration",
    "type": "wordpress-plugin",
    "keywords": ["wordpress", "plugin", "catalog", "google-sheets"],
    "license": "MIT",
    "authors": [
        {
            "name": "Yevhenii Starychenko",
            "email": "evgeniistarychenko@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CatalogMaster\\": "includes/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=WordPress includes/",
        "phpstan": "phpstan analyse includes/"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
} 